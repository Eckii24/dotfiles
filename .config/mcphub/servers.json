{
  "mcpServers": {
    "ado": {
      "args": [
        "-y",
        "@azure-devops/mcp",
        "${AZURE_DEVOPS_ORG}"
      ],
      "autoApprove": [],
      "command": "npx",
      "disabled": false
    },
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "autoApprove": [
        "resolve-library-id",
        "get-library-docs"
      ],
      "command": "npx",
      "disabled": false
    },
    "mcp-promptregistry": {
      "args": [
        "mcp-promptregistry"
      ],
      "command": "npx",
      "disabled": false,
      "env": {
        "PROMPT_REGISTRY_PROJECT_DIR": ""
      }
    },
    "repomix": {
      "args": [
        "--mcp"
      ],
      "autoApprove": [
        "pack_codebase",
        "pack_remote_repository",
        "read_repomix_output",
        "grep_repomix_output",
        "file_system_read_file",
        "file_system_read_directory"
      ],
      "command": "repomix",
      "disabled": false
    },
    "sequential-thinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "autoApprove": [
        "sequentialthinking"
      ],
      "command": "npx",
      "disabled": false
    },
    "serena": {
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant"
      ],
      "command": "uvx",
      "disabled": true
    },
    "vectorcode": {
      "autoApprove": [
        "ls",
        "query"
      ],
      "command": "vectorcode-mcp-server",
      "disabled": false
    }
  },
  "nativeMCPServers": {
    "mcphub": {
      "disabled": true
    },
    "neovim": {
      "disabled": true
    }
  }
}
