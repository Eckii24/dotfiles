{
  "nativeMCPServers": [
    
  ],
  "mcpServers": {
    "vectorcode": {
      "command": "vectorcode-mcp-server",
      "autoApprove": [
        "ls",
        "query"
      ]
    },
    "repomix": {
      "autoApprove": [
        "pack_codebase",
        "pack_remote_repository",
        "read_repomix_output",
        "grep_repomix_output",
        "file_system_read_file",
        "file_system_read_directory"
      ],
      "args": [
        "--mcp"
      ],
      "command": "repomix"
    },
    "ado": {
      "autoApprove": [
        
      ],
      "args": [
        "-y",
        "@azure-devops/mcp",
        "${AZURE_DEVOPS_ORG}"
      ],
      "command": "npx"
    },
    "context7": {
      "autoApprove": [
        "resolve-library-id",
        "get-library-docs"
      ],
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "command": "npx"
    },
    "sequential-thinking": {
      "autoApprove": [
        "sequentialthinking"
      ],
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx"
    },
    "git": {
      "args": [
        "mcp-server-git"
      ],
      "command": "uvx"
    }
  }
}